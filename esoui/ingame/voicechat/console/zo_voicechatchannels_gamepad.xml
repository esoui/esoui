<GuiXml>
    <Controls>
        <Control name="ZO_VoiceChatChannelsEntryGamepad" inherits="ZO_GamepadNewMenuEntryTemplate" virtual="true">
            <Controls>
                <Label name="$(parent)Label" override="true">
                    <Dimensions x="ZO_GAMEPAD_DEFAULT_LIST_ENTRY_WIDTH_VOICECHAT"/>
                </Label>
                <Texture name="$(parent)Icon" override="true">
                    <Anchor point="CENTER" relativeTo="$(parent)Label" relativePoint="RIGHT" offsetX="ZO_GAMEPAD_DEFAULT_LIST_ENTRY_ICON_X_OFFSET_VOICECHAT" />
                </Texture>
                <Texture name="$(parent)StatusIndicator" override="true">
                    <Dimensions x="ZO_GAMEPAD_DEFAULT_LIST_ENTRY_ICON_DIMENSION" y="ZO_GAMEPAD_DEFAULT_LIST_ENTRY_ICON_DIMENSION" />
                    <Anchor point="CENTER" relativeTo="$(parent)Label" relativePoint="RIGHT" offsetX="ZO_GAMEPAD_DEFAULT_LIST_ENTRY_SECOND_ICON_X_OFFSET_VOICECHAT" />
                </Texture>
            </Controls>
        </Control>

        <Control name="ZO_VoiceChatHistoryEntryGamepad" inherits="ZO_GamepadMenuEntryTemplate" virtual="true">
            <Controls>
                <Label name="$(parent)Label" override="true" modifyTextType="NONE">
                    <Dimensions x="ZO_GAMEPAD_DEFAULT_LIST_ENTRY_WIDTH_VOICECHAT_HISTORY"/>
                </Label>
                <Texture name="$(parent)Icon" override="true">
                    <Anchor point="CENTER" relativeTo="$(parent)Label" relativePoint="RIGHT" offsetX="ZO_GAMEPAD_DEFAULT_LIST_ENTRY_ICON_X_OFFSET_VOICECHAT" />
                </Texture>
            </Controls>
        </Control>
    </Controls>
</GuiXml>
